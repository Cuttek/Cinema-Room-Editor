@page "/testPage"
@using CinemaCreatorApp.Components
<h3>testPage</h3>
@for(int i=0;i<3;i++)
{
    int iCopy = i;
    cellRefs.Add(new Cell());
    <Cell @ref="cellRefs[iCopy]" row="i" column="1"></Cell>
}

@code {
    Cell cellRef;
    List<Cell> cellRefs = new List<Cell>();
    void bruh()
    {
        Console.WriteLine(cellRefs);
    }
    protected override void OnAfterRender(bool firstRender)
    {
        if(firstRender)
        {
            foreach(Cell reference in cellRefs)
            {
                Console.WriteLine(reference.row);
            }
            Console.WriteLine(cellRef);

        }
    }
}
